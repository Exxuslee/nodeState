          </QueryParameter>
          <QueryParameter Name="@param_end">
            <Value>=Parameters!param_end.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select *,
 case state  
	when 0 then 'white'
	when 1 then 'green'
	when 2 then 'yellow'
	when 3 then 'red'
 end as colorCell
from  QRSQL
where guild= @param_guild 
      and  unit=@param_unit 
      and label=@param_uzel
      and state=@param_state
      and  Cast(dateTim as Date)&gt;=@param_beg 
      and  Cast(dateTim as Date)&lt;=@param_end 
order by  dateTim</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
